@model List<EssentialOilsCapstone.Models.Property>

    <div class="px-5 py-5">
        <h1>Oil Properties</h1><br />

        @if (Model.Count == 0)
        {
            <p>No properties added yet.</p>
        }
        else
        {
            <table class="table">
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                </tr>
                @foreach (var p in Model)
                {
                    <tr>
                        <td>@p.Id</td>
                        <td>@p.Name</td>
                    </tr>
                }
            </table>
        }
    </div>